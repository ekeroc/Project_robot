<!DOCTYPE html>
<html>
<head>
	<%- include partial/head.ejs %>
	<script type="text/javascript" src="/javascripts/check.js"></script>
	<script type="text/javascript" src="/project/js/updateProject.js"></script>	
</head>
<body> 
	<%- include partial/header.ejs %>
	<div class="section-nav">
		<div class="container">
			<div class="pro_hvcenter_title">
				<h3>
					<ul class="breadcrumb">
						<li><a href="/projectList">Project List</a></li>
						<li class="active"><%=title%></li>
					</ul>
				</h3>
			</div>
		</div>
		<hr>
	</div>
	<div class="container">
		<div class="form-field">
			<form id="projectForm" class="form-horizontal">
				<fieldset>
					<div class="form-group" id="projNameDiv">
						<label class="col-lg-3 control-label">
							<div class="form-label">Project Name:</div>
						</label>
						<div class="col-lg-9">
							<div class="form-label">
								<input type="text" class="form-control" id="projName" name="projName" value="<%= proj.proj_name %>">
							</div>
						</div>
					</div>
					<div class="form-group">
						<label class="col-lg-3 control-label">Project Description:</label>
						<div class="col-lg-9">
							<input type="text" class="form-control" id="projDesc" name="projDesc" value="<%= proj.proj_description %>">
						</div>
					</div>

				</fieldset>
			</form>
			<div class="form-group">
				<div class="col-lg-12">
					<div class="form-btn">
						<button value = "submit" onClick="javascript:history.back(1)" id="cancelTestTaskBtn" class="btn btn-primary">Cancel     </button> 
						<button onClick="" class="btn btn-primary" id="projectEditSubmit" name="projectEditSubmit">Finish</button>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="support">
		<input type="text" id="projID" value="<%= proj._id %>" style="display:none"></p>
	</div>
</body>
</html>