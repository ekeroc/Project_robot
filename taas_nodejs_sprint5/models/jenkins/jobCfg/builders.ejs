<% if (!vars.builders) { %>
    <builders />
<% } else { %>
    <builders>
    <% vars.builders.forEach(function(builder) { %>
        <% if (builder.type == 'shell') { %>
        <hudson.tasks.Shell>
            <command><%= builder.command %></command>
        </hudson.tasks.Shell>
        <% } %>
    <% }); %>
    </builders>
<% } %>
