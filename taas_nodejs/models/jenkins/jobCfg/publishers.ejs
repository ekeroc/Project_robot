<% if (!vars.publishers) { %>
    <publishers />
<% } else { %>
    <publishers>
    <% vars.publishers.forEach(function(publisher) { %>
        <% if (publisher.type == 'cloneWorkspace') { %>
        <hudson.plugins.cloneworkspace.CloneWorkspacePublisher plugin="clone-workspace-scm">
            <workspaceGlob>**/*</workspaceGlob>
            <criteria><%= publisher.criteria? publisher.criteria : 'Not Failed' %></criteria>
            <archiveMethod>TAR</archiveMethod>
            <overrideDefaultExcludes>false</overrideDefaultExcludes>
        </hudson.plugins.cloneworkspace.CloneWorkspacePublisher>
        <% } %>
    <% }); %>
    </publishers>
<% } %>
